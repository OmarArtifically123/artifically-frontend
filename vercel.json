{
  "version": 2,
  "builds": [
    { "src": "server/server.js", "use": "@vercel/node" },
    { "src": "dist/client/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "server/server.js" }
  ],
  "env": {
    "NODE_VERSION": "20",
    "VITE_API_URL": "https://api.artifically.com"
  }
}
