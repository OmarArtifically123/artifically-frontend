    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
