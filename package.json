{
  "name": "artifically-frontend",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Enterprise AI Automation Platform Frontend",
  "keywords": [
    "AI",
    "automation",
    "enterprise",
    "react",
    "vite",
    "ssr"
  ],
  "scripts": {
    "build:tokens": "node scripts/design-system/build-tokens.mjs",
    "dev": "vite --host 0.0.0.0",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "marketing:dev": "astro dev --root marketing --host 0.0.0.0",
    "marketing:build": "astro build --root marketing",
    "marketing:preview": "astro preview --root marketing",
    "build": "npm run build:tokens && npm run marketing:build && npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node server/server.js",
    "ssr:dev": "NODE_ENV=development node server/server.js",
    "ssr:start": "NODE_ENV=production node server/server.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host 0.0.0.0",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "storybook:chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --build-script-name=storybook:build",
    "storybook:test": "test-storybook",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "postinstall": "node scripts/assets/materialize-static-assets.mjs && partytown copylib public/~partytown",
    "security:scan": "npm audit --audit-level=moderate",
    "report:bundle": "node scripts/perf/bundle-report.mjs",
    "report:lighthouse": "node scripts/perf/frontend-lighthouse.mjs",
    "ci:lighthouse": "lhci autorun"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.0",
    "cookie": "^1.0.2",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.22",
    "gsap": "^3.12.5",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.363.0",
    "million": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-router-dom": "^6.8.0",
    "three": "^0.160.1",
    "use-debounce": "^10.0.6",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@gltf-transform/cli": "^4.2.1",
    "@playwright/test": "^1.42.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jsdom": "^21.1.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@storybook/addon-a11y": "^8.1.5",
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-interactions": "^8.1.5",
    "@storybook/react-vite": "^8.1.5",
    "@storybook/test-runner": "^0.18.2",
    "@astrojs/node": "^8.2.1",
    "@astrojs/react": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.5.0",
    "chromatic": "^11.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^1.7.1",
    "jsdom": "^24.0.0",
    "lighthouse": "^12.1.0",
    "puppeteer": "^24.23.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "astro": "^4.10.2",
    "storybook": "^8.1.5",
    "terser": "^5.26.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
