{
  "name": "artifically-next",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "Enterprise AI Automation Platform Frontend",
  "keywords": [
    "AI",
    "automation",
    "enterprise",
    "react",
    "nextjs"
  ],
  "scripts": {
    "build:tokens": "node scripts/design-system/build-tokens.mjs",
    "dev": "next dev",
    "build": "npm run build:tokens && next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "storybook:chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --build-script-name=storybook:build",
    "storybook:test": "test-storybook",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:a11y": "vitest run tests/accessibility/design-system.a11y.test.tsx",
    "test:e2e": "playwright test",
    "postinstall": "node scripts/assets/materialize-static-assets.mjs && partytown copylib public/~partytown",
    "assets:hero": "node scripts/assets/export-hero-images.mjs",
    "security:scan": "npm audit --audit-level=moderate",
    "report:bundle": "node scripts/perf/bundle-report.mjs",
    "report:lighthouse": "node scripts/perf/frontend-lighthouse.mjs",
    "report:webpagetest": "node scripts/perf/webpagetest-baseline.mjs",
    "report:dashboard": "node scripts/perf/build-dashboard.mjs",
    "ci:lighthouse": "lhci autorun --config=./lighthouserc.js"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-virtual": "^3.13.12",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.0",
    "cookie": "^1.0.2",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.24",
    "gsap": "^3.12.5",
    "lodash-es": "^4.17.21",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.363.0",
    "million": "^3.0.6",
    "next": "14.2.15",
    "next-themes": "^0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.0.11",
    "react-intersection-observer": "^9.13.1",
    "react-router-dom": "^6.30.1",
    "react-spring": "^10.0.3",
    "three": "^0.160.1",
    "use-debounce": "^10.0.6",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@astrojs/node": "^8.2.1",
    "@astrojs/react": "^3.3.1",
