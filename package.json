{
  "name": "artifically-frontend",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Enterprise AI Automation Platform Frontend",
  "keywords": [
    "AI",
    "automation",
    "enterprise",
    "react",
    "vite",
    "ssr"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr server/entry-server.jsx --outDir dist/server",
    "build": "npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node server/server.js",
    "ssr:dev": "NODE_ENV=development node server/server.js",
    "ssr:start": "NODE_ENV=production node server/server.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host 0.0.0.0",
    "type-check": "tsc --noEmit",
    "postinstall": "partytown copylib public/~partytown"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "axios": "^1.6.0",
    "framer-motion": "^12.23.22",
    "gsap": "^3.12.5",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.1",
    "million": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-router-dom": "^6.8.0",
    "three": "^0.160.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rollup-plugin-visualizer": "^6.0.4",
    "terser": "^5.26.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
